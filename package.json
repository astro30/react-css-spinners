{
  "name": "react-css-spinners",
  "version": "1.0.0",
  "description": "CSS spinners for React",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "ts-node-dev --transpileOnly --no-notify src",
    "lint": "npx eslint '**/*.{ts,jsx,js}'",
    "lint:fix": "npx prettier '**/*.{ts,jsx,js}' --write",
    "build": "rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astro30/react-css-spinners.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "astro30",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astro30/react-css-spinners/issues"
  },
  "homepage": "https://github.com/astro30/react-css-spinners#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.0",
    "rollup-plugin-terser": "^5.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@types/react": "^16.9.35",
    "react": "^16.13.1"
  }
}
